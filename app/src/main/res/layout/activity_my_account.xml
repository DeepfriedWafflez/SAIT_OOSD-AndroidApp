<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MyAccountActivity">

    <LinearLayout
        android:id="@+id/linearLayout10"
        android:layout_width="199dp"
        android:layout_height="508dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="8dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/linearLayout11"
        app:layout_constraintTop_toTopOf="parent">

        <EditText
            android:id="@+id/etCustomerId"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="Customer Id" />

        <EditText
            android:id="@+id/etCustFirstName"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="First Name" />

        <EditText
            android:id="@+id/etCustLastName"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="Last Name" />

        <EditText
            android:id="@+id/etCustAddress"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="Address" />

        <EditText
            android:id="@+id/etCustCity"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="City" />

        <EditText
            android:id="@+id/etCustProv"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="Province" />

        <EditText
            android:id="@+id/etCustPostal"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="Postal Code" />

        <EditText
            android:id="@+id/etCustCountry"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="Country" />

        <EditText
            android:id="@+id/etCustHomePhone"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="Home Phone" />

        <EditText
            android:id="@+id/etCustBusPhone"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="Business Phone" />

        <EditText
            android:id="@+id/etCustEmail"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="Email" />

        <EditText
            android:id="@+id/etAgentId"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:editable="false"
            android:text="Agent Id" />

        <EditText
            android:id="@+id/etCustUsername"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="User Name" />

        <EditText
            android:id="@+id/etCustPassword"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:editable="false"
            android:text="Password" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="186dp"
        android:layout_height="498dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="70dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Customer Id" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="First Name" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Last Name" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Address" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="City" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Province" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Postal Code" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Country" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Home Phone" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Business Phone" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Email" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:editable="false"
            android:text="Agent Id" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="User Name" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Password" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout13"
        android:layout_width="395dp"
        android:layout_height="86dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="37dp"
        android:orientation="horizontal"
        android:padding="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout10">

        <Button
            android:id="@+id/btnEdit"
            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Edit Account" />

        <Button
            android:id="@+id/btnBooking"
            android:layout_width="49dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Booking History" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="142dp"
        android:layout_marginTop="16dp"
        android:text="My Account"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>